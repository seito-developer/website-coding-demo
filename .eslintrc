{
    "extends": [
      // "eslint:recommended",
      "eslint-config-prettier"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "rules": {
      "no-unused-vars": [
        "error"
      ],
      "no-undef": "error"
    }
  }